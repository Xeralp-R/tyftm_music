  \clef treble

  \partial 8
  e'8 |
  e'4. d'8 |
  e'8 fis'4 e'8 |
  e'8 d'8 d'8 d'16 cis'16 |
  % 5
  d'4. d'8 |
  b'8 a'8 g'8. fis'16 |
  a'8 g'8 fis'8. d'16 |
  g'4~ g'16 fis'16 e'16 d'16 |
  e'4. e'8 |
  % 10
