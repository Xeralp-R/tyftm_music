piano_rh = {
  \key d \major
  \time 2/4
  \clef treble

  \partial 8
  <e' cis'' >8 |
  <fis' d'' >2 |
  <fis' d'' >2 |
  <fis' b' d'' >2 |
  % 5
  <fis' b' d'' >2 |
  <g' b' d'' >2 |
  <fis' a' d'' >2 |
  <e' g' b' >4 <e' g' b' >4 |
  <e' a' cis'' >2 |
  % 10
  <fis' ais' cis'' >2 |
  <fis' b' d'' >2 |
  <fis' ais' cis'' >2 |
  <d' fis' b' >2 |
  <g' b' d'' >2 |
  % 15
  <fis' a' d'' >2 |
  <e' g' b' >2 |
  <<
    \context Voice = "voiceone" {
      \voiceOne
      <a' e'' >2~~ |
      <a' e'' >2 |
    }
    \context Voice = "voicetwo" {
      \voiceTwo
      d''2 |
      <g' cis'' >2 |
    }
  >>
}

piano_lh = {
  \key d \major
  \time 2/4
  \clef bass

  \partial 8
  r8 |
  d,8 d8 a,8 fis,8 |
  cis,4 <cis cis' >4 |
  b,8 d8 fis8 b8 |
  % 5
  <a, a >8 a,8 <fis, fis >8 fis,8 |
  <g, g >4. g8 |
  <fis, fis >4. fis8 |
  <e, e >4 <d, d >4 |
  <a, a >4. a8 |
  % 10
  fis,8 cis8 fis8 ais8 |
  b4. g,8 |
  fis,8 ais,8 cis8 fis8 |
  <b, b >2 |
  g,8 b,8 d8 g8 |
  % 15
  <fis, fis >2 |
  e,8 g,8 b,8 e8 |
  <a, a >2 |
  <a, a >2 |
}

iesu_panis_vitae_verse_one = {
  <<
    \tag #'(full_chorus solo soprano)
    \new Staff \with {
      midiInstrument = "voice oohs"
      instrumentName = "Soprano Solo"
      shortInstrumentName = "S-solo"
    } <<
      \new Voice = "sop_solo" {
        \verse_one \bar "||" \mark "To Chor."
      }
      \new Lyrics \lyricsto "sop_solo" \verse_one_lyr
    >>

    \tag #'(accompaniment solo piano)
    \new PianoStaff \with {
      instrumentName = "Piano"
      shortInstrumentName = "Pf."
      midiInstrument = "acoustic grand"
      connectArpeggios = ##t
      \mergeDifferentlyDottedOn
      \mergeDifferentlyHeadedOn
    }
    <<
      \new Staff = "1" { \piano_rh \bar "||" \mark "To Chor." }
      \new Staff = "2" { \piano_lh \bar "||" \mark "To Chor." }
    >>
  >>
}

