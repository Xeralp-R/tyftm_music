glory_to_god = \score {
  \header {
    composer = "Fr. Manoling Francisco, SJ"
    subtitle = "Gloria"
    title = "Glory to God"
  }
  <<
    \tag #'full_chorus \tag #'cantor_solo
    \new Staff \with {
      instrumentName = "Cantor"
      shortInstrumentName = "Ctr."
      midiInstrument = "voice oohs"
    }
    <<
      \new Voice = "PartPOneVoiceOne" { \part-Pone-one }
      \new Lyrics \lyricsto "PartPOneVoiceOne" { \PartPOneVoiceOneLyricsOne }
    >>

    \tag #'full_chorus \tag #'chorus
    \new ChoirStaff \with {
      instrumentName = "Choir"
      midiInstrument = "choir aahs"
    } <<

      \tag #'soprano_chorus
      \new Staff \with {
        instrumentName = "Soprano"
        shortInstrumentName = "S."
      } <<
        \new Voice = "PartPTwoVoiceOne" { \part-Ptwo-one }
        \new Lyrics \lyricsto "PartPTwoVoiceOne" { \chor_alt_lyr }
      >>

      \tag #'alto_chorus
      \new Staff \with {
        instrumentName = "Alto"
        shortInstrumentName = "A."
      } <<
        \new Voice = "alto" { \part-Pthree-one }
        \new Lyrics \lyricsto "alto" { \chor_lyr }
      >>

      \tag #'tenor_chorus
      \new Staff \with {
        instrumentName = "Tenor"
        shortInstrumentName = "T."
      } <<
        \new Voice = "tenor" { \part-Pfour-one }
        \new Lyrics \lyricsto "tenor" { \chor_alt_lyr }
      >>

      \tag #'bass_chorus
      \new Staff \with {
        instrumentName = "Bass"
        shortInstrumentName = "B."
      } <<
        \new Voice = "bass" { \part-Pfive-one }
        \new Lyrics \lyricsto "bass" { \chor_lyr }
      >>
    >>

    \tag #'accompaniment \tag #'soli
    \new ChoirStaff <<
      \tag #'asax_solo
      \new Staff \with {
        instrumentName = "Alto Saxophone"
        shortInstrumentName = "Asax."
        midiInstrument = "alto sax"
