        midiInstrument = "acoustic grand"
        connectArpeggios = ##t
      }
      <<
        \new Staff = "1" { \part-Pseven-one }
        \new Staff = "2" { \part-Pseven-two }
      >>
    >>

    \tag #'accompaniment \tag #'tutti
    \new StaffGroup <<

      \tag #'violins
      \new StaffGroup \with {
        systemStartDelimiter = #'SystemStartSquare
        midiInstrument = "violin"
      } <<

        \tag #'violin_i
        \new Staff \with {
